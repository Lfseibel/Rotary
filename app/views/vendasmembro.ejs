<!DOCTYPE html>
<html>
<head>
	<meta name="viewport" content="width=device-width", initial-scale="1.0">
	<link rel="stylesheet" type="text/css" href="/css/addmembro.css">	
	<link href="https://fonts.googleapis.com/css?family=Merriweather+Sans" rel="stylesheet">
</head>
<body>
	<header>
		<div id="top">
			<div id="branding"><h1 class="texto" id="titulo">Rotary</h1>
			<img class="logo" src="img/rotarylogo.png"></div>
		</div>
	</header>
	
    <% if(vendas.length > 0) { %>
        <% for(var i = 0; i < vendas.length; i++) { %>
            <li>
                <a href="venda?id_venda=<%= vendas[i].id_venda %>">
                  <p><%= vendas[i].evento %></p>
                  <p><%= vendas[i].quantidade_ingressos %></p>
                  <p><%= vendas[i].valor_ingresso %></p>
                  <p><%= vendas[i].vendas_realizadas %></p>
                </a>
            </li> 
          <% } %>
        <% } %>

	<footer>
		<a href="#">Est√°girarios IFMS Campus Dourados &copy; 2019</a>
	</footer>
	<script type="text/javascript" src="js/main.js"></script>
</body>	
</html>